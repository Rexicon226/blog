digraph G {
  compound=true
  clusterrank=local
  graph [fontsize=14 compound=true]
  node [shape=box, style=filled];
  rankdir=BT;
  ordering="in";
  bgcolor="#2e2e2e"

  subgraph cluster_1 {
    style=dotted
    color="orange"
    penwidth=3
    1.0 [label="10", color="red"];
  }
  subgraph cluster_2 {
    style=dotted
    color="orange"
    penwidth=3
    2.0 [label="20", color="red"];
  }
  subgraph cluster_0 {
    style=dotted
    color="orange"
    penwidth=3
    0.0 [label="add", color="green"];
    0.1 [label="30", color="green"];
  }

  0.0 -> 1.0 [lhead = cluster_1 color="lightblue"]
  0.0 -> 2.0 [lhead = cluster_2 color="lightblue"]
}
